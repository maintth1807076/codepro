<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>

    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <script src="/js/jquery-3.3.1.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
</head>
<body>
<h1>List product</h1>
<div class="container ">
    <div >
        <%
        for(var i = 0; i < listCustomer.length; i++){
        %>
            <div class="item-customer">
                <div>Id: <%= listCustomer[i].customerId%></div>
                <div>Username: <%= listCustomer[i].name%></div>
                <div>Password: <%= listCustomer[i].password%></div>
                <div>Email: <%= listCustomer[i].email%></div>
                <div>Address: <%= listCustomer[i].address%></div>
                <div>Phone: <%= listCustomer[i].phone%></div>
            </div>
        <%
        };
        %>
    </div>

</div>

</body>
</html>
                            <label for="registerInputPhone">Phone</label>
                            <input type="text" name="phone" class="form-control" id="registerInputPhone"
                                   placeholder="Phone">
                        </div>
                        <div class="form-group">
                            <i class="fas fa-address-card"></i>
                            <label for="registerInputAddress">Address</label>
                            <textarea name="address" id="registerInputAddress" cols="50" rows="5"></textarea>
                        </div>
                        <div class="form-inline">
                            <i class="fas fa-address-card"></i>
                            <label for="registerInputAvatar">Avatar</label>
                            <input type="text" name="avatar" class="form-control" id="registerInputAvatar"
                                   placeholder="Avatar">
                        </div>
                        <div class="form-inline">
                            <i class="fa fa-envelope prefix"></i>
                            <label for="InputEmail2">Email address</label>
                            <input type="email" name="email" class="form-control" id="InputEmail2"
                                   aria-describedby="email2" placeholder="Enter email">
                            <small id="email2" class="form-text text-muted">We'll never share your email with anyone
                                else.
                            </small>
                        </div>
                        <div class="form-inline">
                            <i class="fa fa-lock prefix"></i>
                            <label for="InputPassword2">Password</label>
                            <input type="password" name="password" class="form-control" id="InputPassword2"
                                   placeholder="Password">
                        </div>

                        <div class="form-inline">
                            <i class="far fa-calendar-alt"></i>
                            <label for="inputBirthday">Birthday</label>
                            <input type="date" class="form-group" id="inputBirthday" name="birthday">
                        </div>

                        <div class="form-inline">
                            <label>&nbsp;</label>
                            <button type="button" class="btn" data-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary" id="btn-Submit" value="submit">Sign Up</button>
                        </div>
                    </form>
